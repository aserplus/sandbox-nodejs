{
    "env": {
        "node": true,
        "mocha": true
    },
    "parserOptions":{
        "ecmaVersion": 2017
    },
    "rules": {
        "linebreak-style": 0,
        "react/jsx-indent-props": [1, 4],
        "comma-dangle": [2, "always-multiline"],
        "eol-last": [0],
        "indent": [1, 4],
        "one-var": [2, { "var": "always", "let": "never", "const": "never" }],
        "max-len": [2, {"code": 128}]
    },
    "extends": "airbnb"
}